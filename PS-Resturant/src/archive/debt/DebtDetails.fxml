<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.VBox?>


<ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false" styleClass="root" stylesheets="@../../resources/style/base.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="archive.debt.DebtDetailsController">
   <content>
      <VBox alignment="CENTER">
         <children>
            <JFXTreeTableView fx:id="debtTable" minHeight="400.0" minWidth="650.0" styleClass="table" VBox.vgrow="ALWAYS">
               <columnResizePolicy>
                  <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TreeTableColumn fx:id="debtAmount_Col" prefWidth="75.0" text="قيمة الآجل" />
                  <TreeTableColumn fx:id="username_Col" prefWidth="75.0" text="الكاشير" />
                  <TreeTableColumn fx:id="billNumber_Col" prefWidth="75.0" text="رقم الفاتورة" />
               </columns>
            </JFXTreeTableView>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
